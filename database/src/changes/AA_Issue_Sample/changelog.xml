<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
	http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        logicalFilePath="/com/onbelay/dealcapture/change/ISSUE-xxxxx-Changes.xml"
>
    <!--
        Point to other change log files that contain the change(s)
        This gives us flexibility to add future changes without changing
        the MD5 checksum for changes that have already run.
    -->
    <include file="ISSUE-xxxxx/ShortDescriptionOfChangeOne.xml" relativeToChangelogFile="true"/-->
    <include file="ISSUE-xxxxx/ShortDescriptionOfChangeTwo.xml" relativeToChangelogFile="true"/-->
</databaseChangeLog>
