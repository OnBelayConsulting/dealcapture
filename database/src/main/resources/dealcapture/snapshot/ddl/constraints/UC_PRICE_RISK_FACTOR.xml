        <createIndex tableName="PRICE_RISK_FACTOR" indexName="PRICE_RISK_FACTOR_INDEX_IDX">
            <column name="PRICE_INDEX_ID"/>
        </createIndex>

    </changeSet>

</databaseChangeLog>
