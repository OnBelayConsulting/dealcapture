                tableName='PRICE_RISK_FACTOR'
                constraintName='PRICE_RISK_FACTOR_UC'/>
    </changeSet>
    <changeSet author="lefeuvrem" id="2.0" objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

        <createIndex tableName="PRICE_RISK_FACTOR" indexName="PRICE_RISK_FACTOR_DATE_IDX">
